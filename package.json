{
  "name": "school4u",
  "version": "1.0.0",
  "description": "Educational app",
  "main": "main.js",
  "scripts": {
    "install-all": "npm install && cd src && npm install",
    "start-react": "cd src && npm start && cd ..",
    "build-react": "cd src && npm run build && cd ..",
    "start": "npm run build-react && electron .",
    "start-forge": "electron-forge start",
    "format": "",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64,arm64 --icon=./src/icons/icon.icns --prune=true --out=release-builds",
    "build-win": "electron-packager . Schoo4u --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./src/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"School4u\"",
    "build-lin": "electron-packager . Schoo4u --overwrite --asar=true --platform=linux --arch=x64 --icon=./src/icons/icon.png --prune=true --out=release-builds",
    "build-packager": "npm run build-win && npm run build-mac && npm run build-lin",
    "build-forge": "electron-forge package",
    "make-forge": "electron-forge make"
  },
  "repository": "git+https://github.com/B-P-Coders/coding-night.git",
  "keywords": [
    "education",
    "planning",
    "management"
  ],
  "author": "B&P Coders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/B-P-Coders/coding-night/issues"
  },
  "homepage": "https://github.com/B-P-Coders/coding-night#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "electron-forge": "^5.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "missing": "^0.0.1",
    "npm-install-missing": "^0.1.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^16.0.1",
    "electron-prebuilt-compile": "^8.2.0",
    "foreman": "^3.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "school4u"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./src/icons/banner.png",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "maintainer": "B&P Coders",
            "homepage": "https://school4u.ml/"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "maintainer": "B&P Coders",
            "homepage": "http://school4u.ml/"
          }
        }
      ]
    }
  }
}
