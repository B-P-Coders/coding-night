{
  "name": "school4u",
  "version": "1.0.0",
  "description": "Educational app",
  "main": "main.js",
  "scripts": {
    "start-react": "cd src && npm start",
    "build-react": "cd src && npm run build",
    "start": "npm run build-react && electron .",
    "format": "",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64,arm64 --icon=icons/icon.icns --prune=true --out=release-builds",
    "build-win": "electron-packager . Schoo4u --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"School4u\"",
    "build-lin": "electron-packager . Schoo4u --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/icon.png --prune=true --out=release-builds",
    "build-packager": "npm run build-win && npm run build-mac && npm run build-lin",
    "build-builder": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/B-P-Coders/coding-night.git"
  },
  "keywords": [
    "education",
    "planning",
    "management"
  ],
  "author": "B&P Coders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/B-P-Coders/coding-night/issues"
  },
  "homepage": "https://github.com/B-P-Coders/coding-night#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "missing": "^0.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "electron-packager": "^15.4.0",
    "foreman": "^3.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1"
  }
}
